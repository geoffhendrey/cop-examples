{
    "kind": "extension",
    "namespace": {
      "name": "@INSTALL ${$sys.solutionId}",
      "version": 1
    },
    "name": "security.risk_score.extension",
    "displayName": "Security risk score extension",
    "description": "Extends k8s:workload to add a security risk score metric",
    "extends": {
      "k8s": [
        "workload"
      ]
    },
    "metricTypes": [
        "@INSTALL ${$sys.solutionId & ':security.risk_score'}",
    ]
  }