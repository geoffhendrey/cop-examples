{
    "manifestVersion": "1.1.0",
    "name": "SOLUTION_PREFIXmeteodata",
    "solutionVersion": "0.1.0",
    "solutionType": "component",
    "dependencies": [
        "fmm",
        "zodiac",
        "iam",
        "logs"
    ],
    "description": "OpenMeteo data model and collector",
    "contact": "pbucek@cisco.com",
    "homepage": "https://github.com/geoffhendrey/cop-examples/tree/main/example/meteodata",
    "gitRepoUrl": "https://github.com/geoffhendrey/cop-examples/tree/main/example/meteodata",
    "readme": "https://github.com/geoffhendrey/cop-examples/blob/main/example/meteodata/README.md",
    "objects": [
        {
            "type": "fmm:namespace",
            "objectsDir": "objects/model/namespaces"
        },
        {
            "type": "fmm:entity",
            "objectsDir": "objects/model/entities"
        },
        {
            "type": "fmm:metric",
            "objectsDir": "objects/model/metrics"
        },
        {
            "type": "fmm:resourceMapping",
            "objectsDir": "objects/model/resource-mappings"
        },
        {
            "type": "zodiac:function",
            "objectsDir": "objects/functions/function"
        },
        {
            "type": "zodiac:subscriptionCronConfig",
            "objectsDir": "objects/functions/subscriptionCronConfig"
        },
        {
            "type": "zodiac:egressHosts",
            "objectsDir": "objects/functions/egressHosts"
        },
        {
            "type": "iam:Permission",
            "objectsDir": "objects/iam/permission"
        },
        {
            "type": "iam:RoleToPermissionMapping",
            "objectsDir": "objects/iam/roleToPermissionMapping"
        },
        {
            "type": "SOLUTION_PREFIXmeteodata:meteoConfig",
            "objectsDir": "objects/meteodata/meteoConfig"
        }
    ],
    "types": [
        "types/meteo_location.json",
        "types/meteo_config.json"
    ]
}
